---
layout: layouts/base.njk
---
<header>
    <h1>
        <a href="/">Tanga Yacht Club</a>
    </h1>

    {% include "hamburger.njk" %}

</header>

<img class="post-splash" src="{{ featuredImage | url }}" alt="{{ featuredImageTitle }}" ></img>

<main >
    <article class="times">
        <div class="wrapper">
            <div class="content">
                <h1>{{ title }}</h1>

                <h6>{% fullBuildDate %}</h6>

                {# <p>{% for tag in tags %}<span class="tag">{{ tag }}</span>{% endfor %}</p> #}

                {{ content | safe }}

            </div>
        </div>
    </article>
    <aside>
        {% include "aside.njk" %}
    </aside>
</main>